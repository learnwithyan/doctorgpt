{
  "name": "doctorgpt",
  "displayName": "DoctorGPT fix your code",
  "description": "The DoctorGPT fix your developer code and brings the power of AI-driven code completion and suggestions right into your coding environment. This extension enhances your coding experience by providing intelligent suggestions, snippets, and code completions based on the context of your code.",
  "version": "1.3.0",
  "publisher": "learnwithyan",
  "icon": "images/logo_128.png",
  "bugs": {
    "url": "https://github.com/learnwithyan/doctorgpt",
    "email": "learnwithyan@hotmail.com"
  },
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "doctorgpt.com1",
        "title": "%doctorgpt.com1.title%",
        "icon": "$(sort-precedence)"
      },
      {
        "command": "doctorgpt.com2",
        "title": "%doctorgpt.com2.title%",
        "icon": "$(sort-precedence)"
      },
      {
        "command": "doctorgpt.com3",
        "title": "%doctorgpt.com3.title%",
        "icon": "$(sort-precedence)"
      }
    ],
    "configuration": {
      "title": "doctorgpt Configurations",
      "properties": {
        "doctorgpt.api-key": {
          "type": "string",
          "default": "",
          "description": "Set here API key from chat GPT"
        },
        "doctorgpt.bg-chat-color": {
          "type": "string",
          "default": "#000",
          "description": "Set here background color for chat GPT"
        },
        "doctorgpt.bg-text-size": {
          "type": "string",
          "default": "14",
          "description": "Set here text size for chat GPT"
        },
        "doctorgpt.text-primary-color": {
          "type": "string",
          "default": "#2e95d3",
          "description": "Set here Primary color for chat GPT"
        },
        "doctorgpt.text-secondary-color": {
          "type": "string",
          "default": "#00a67d",
          "description": "Set here Secondary color for chat GPT"
        },
        "doctorgpt.code-lang": {
          "type": "string",
          "default": "common",
          "description": "Select your preferred programming language",
          "enum": [
            "actionscript",
            "ada",
            "apex",
            "assembly",
            "clojure",
            "cobol",
            "coffeescript",
            "common",
            "csharp",
            "css",
            "dart",
            "delphi",
            "elixir",
            "erlang",
            "fortran",
            "fsharp",
            "go",
            "groovy",
            "haskell",
            "html",
            "java",
            "javascript",
            "julia",
            "kotlin",
            "lisp",
            "lua",
            "matlab",
            "objective-c",
            "perl",
            "php",
            "powershell",
            "python",
            "r",
            "ruby",
            "rust",
            "scala",
            "scheme",
            "shell",
            "smalltalk",
            "sql",
            "swift",
            "tcl",
            "typescript",
            "vbnet",
            "vbscript",
            "webassembly"
          ]
        }
      }
    }
  },
  "localize": [
    "package.nls.json",
    "package.bg.nls.json",
    "package.cs.nls.json",
    "package.de.nls.json",
    "package.es.nls.json",
    "package.fr.nls.json",
    "package.hu.nls.json",
    "package.it.nls.json",
    "package.ko.nls.json",
    "package.pl.nls.json",
    "package.pt.nls.json",
    "package.ru.nls.json",
    "package.tr.nls.json",
    "package.zh.nls.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/learnwithyan/doctorgpt.git"
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "l10n": "./l10n",
  "jest": {
    "moduleNameMapper": {
      "^vscode$": "<rootDir>/__mocks__/vscode.js"
    }
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "typescript": "^5.3.3"
  }
}
